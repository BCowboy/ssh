<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.lol.lw.entity">
  <class name="UserOrder" table="USERORDER">
    <id name="id" type="int" column="id">
      <generator class="native" />
    </id>
    <property name="quantity" type="int" column="quantity"
      not-null="true" />
       <property name="pro_name" type="string" column="pro_name"
      not-null="true" />
    <!--  <many-to-one name="user"  column="userId" class="User"></many-to-one> -->
  </class>
</hibernate-mapping>
